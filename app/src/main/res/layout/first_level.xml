        android:textColor="@color/red"
        android:textSize="@dimen/vtoroy_rasmer"
        android:textStyle="bold"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
